{
  "name": "@business",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack --progress --mode production",
    "clean": "rm -rf ./build",
    "clean-fbts": "rm i18n/fbt/.enum_manifest.json i18n/fbt/.src_manifest.json i18n/fbt/source_strings.json i18n/fbt/translatedFbts.json 2&> /dev/null || exit 0",
    "clean-fbts:win": "del i18n\\fbt\\.enum_manifest.json i18n\\fbt\\.src_manifest.json i18n\\fbt\\source_strings.json i18n\\fbt\\translatedFbts.json 2>nul || exit 0",
    "collect-fbts": "fbt-collect --fbt-common-path i18n/fbt/common_strings.json --hash-module fb-tiger-hash --react-native-mode --manifest < i18n/fbt/.src_manifest.json > i18n/fbt/source_strings.json",
    "dev": "pnpm webpack serve --progress --mode none",
    "format": "pnpm lint --fix",
    "generate-fbt": "node i18n/scripts/generate-localizables-executor.js",
    "i18n:build": "pnpm manifest && pnpm collect-fbts && pnpm translate-fbts && pnpm generate-fbt",
    "i18n:build:win": "pnpm clean-fbts:win && pnpm manifest && pnpm collect-fbts && pnpm translate-fbts:win && pnpm generate-fbt",
    "lint": "eslint src --ext .js,.ts,.tsx,.jsx --ignore-pattern '!.*.js'",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "manifest": "fbt-manifest --src src/ --enum-manifest i18n/fbt/.enum_manifest.json --src-manifest i18n/fbt/.src_manifest.json",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "relay": "relay-compiler",
    "server:install": "cd ./relay-server && pnpm install",
    "start": "pnpm webpack serve --hot --progress --mode development --stats-children",
    "test": "jest",
    "test:coverage": "rm -rf coverage && jest --coverage && open coverage/lcov-report/index.html",
    "test:watch": "jest --watchAll=true",
    "translate-fbts": "fbt-translate --jenkins --source-strings ./i18n/fbt/source_strings.json --translations i18n/fbt/translationScriptInput/*.json > ./i18n/fbt/translatedFbts.json",
    "translate-fbts:win": "for %i in (i18n\\fbt\\translationScriptInput\\*.json) do fbt-translate --jenkins --source-strings i18n\\fbt\\source_strings.json --translations %i >> i18n\\fbt\\translatedFbts.json",
    "upgrade": "npx npm-check-updates &&  npx npm-check-updates -u",
    "watch": "pnpm webpack serve --progress --mode production",
    "lib": "babel ./src --out-dir ./lib"
  },
  "dependencies": {
    "react": "git://github.com/tienlx97/react-experimental.git#www",
    "react-dom": "git://github.com/tienlx97/react-dom-experimental.git#www",
    "scheduler": "0.22.0",
    "@meta-plugins/configs-prettier": "workspace:*",
    "@meta-plugins/configs-eslint": "workspace:*",
    "@meta-core/utils": "workspace:*"
  },
  "devDependencies": {
    "@babel/cli": "7.25.6",
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "7.23.3",
    "@babel/node": "7.22.19",
    "@babel/plugin-transform-modules-commonjs": "7.24.1",
    "@babel/plugin-transform-private-methods": "7.24.7",
    "@babel/plugin-transform-react-jsx": "7.24.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@stylexjs/babel-plugin": "0.6.1",
    "@stylexjs/dev-runtime": "^0.6.1",
    "@stylexjs/stylex": "0.6.1",
    "@stylexjs/webpack-plugin": "0.6.1",
    "@svgr/webpack": "^6.5.1",
    "@swc/jest": "^0.2.36",
    "@types/node": "^18.11.18",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-router-dom": "5.3.3",
    "babel-loader": "^9.1.2",
    "babel-plugin-fbt": "1.0.0",
    "babel-plugin-fbt-runtime": "1.0.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-relay": "16.1.0",
    "babel-preset-react-app": "^10.0.1",
    "buffer": "6.0.3",
    "clean-webpack-plugin": "4.0.0",
    "confusing-browser-globals": "1.0.11",
    "copy-webpack-plugin": "12.0.2",
    "crypto-browserify": "3.12.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "esbuild-loader": "^2.21.0",
    "fb-tiger-hash": "1.0.0",
    "fbjs": "^3.0.5",
    "fbt": "1.0.0",
    "fbt-generate-translations": "0.0.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "5.6.0",
    "invariant": "2.2.4",
    "js-cookie": "3.0.5",
    "jsresource": "0.0.8",
    "mini-css-extract-plugin": "^2.7.2",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "postcss": "^8.4.31",
    "postcss-loader": "8.1.1",
    "postcss-preset-env": "^7.8.3",
    "react-refresh": "^0.14.2",
    "react-relay": "16.1.0",
    "react-router-dom": "6.23.1",
    "react-strict-dom": "0.0.11",
    "relay-runtime": "16.1.0",
    "resize-observer-polyfill": "1.5.1",
    "source-map-loader": "5.0.0",
    "stream-browserify": "3.0.0",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.10",
    "ts-loader": "^9.4.2",
    "ua-parser-js": "1.0.37",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "3.0.0",
    "yargs": "17.7.2"
  }
}
