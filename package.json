    "lint-staged": "15.2.10",
    "prettier": "^2.8.3",
    "syncpack": "13.0.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "remark": "15.0.1",
    "remark-cli": "12.0.1",
    "rimraf": "6.0.1",
    "@babel/eslint-parser": "7.23.3",
    "@babel/preset-react": "^7.23.3",
    "plop": "4.0.1",
    "node-plop": "0.32.0",
    "inquirer-search-list": "1.2.6"
  }
