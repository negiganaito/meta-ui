{
  "name": "@meta-ui",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf \"**/node_modules\"",
    "fresh": "pnpm clean && pnpm i",
    "nuke": "rimraf pnpm-lock.yaml && pnpm fresh",
    "format": "prettier",
    "format:check": "pnpm format --check --debug-check",
    "format:fix": "pnpm format --write",
    "lint": "eslint --ext mjs,js,ts,tsx",
    "lint:fix": "pnpm lint --fix",
    "commitlint": "commitlint",
    "syncpack:fix": "syncpack format && syncpack fix-mismatches",
    "fix": "pnpm lint:fix . && pnpm remark:fix . && pnpm format:fix . && pnpm syncpack:fix",
    "prepare": "husky install",
    "remark": "remark",
    "remark:fix": "pnpm remark -o --",
    "dev": "cd app/business && pnpm dev",
    "create-component": "plop --plopfile ./generator/plopfile.js --dest ."
  },
  "dependencies": {
    "react": "git://github.com/tienlx97/react-experimental.git#www",
    "react-dom": "git://github.com/tienlx97/react-dom-experimental.git#www",
    "scheduler": "0.22.0",
    "@meta-plugins/configs-commitlint": "workspace:*",
    "@meta-plugins/configs-remark": "workspace:*",
    "@meta-plugins/configs-syncpack": "workspace:*",
    "@babel/eslint-parser": "7.23.3",
    "@stylexjs/eslint-plugin": "0.6.1",
    "@stylexjs/babel-plugin": "0.6.1",
    "eslint": "^8.55.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-function-declare-after-return": "1.1.0",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-compiler": "0.0.0-experimental-0998c1e-20240625",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "confusing-browser-globals": "1.0.11",
    "@babel/preset-react": "^7.23.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@babel/preset-typescript": "^7.18.6"
  },
